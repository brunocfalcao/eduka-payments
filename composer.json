{
    "name": "brunocfalcao/eduka-payments",
    "description": "Eduka payments gateway",
    "keywords": [
        "brunocfalcao"
    ],
    "homepage": "https://github.com/brunocfalcao/eduka-payments",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Bruno Falc√£o",
            "email": "bruno.c.falcao@brunofalcao.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "brunocfalcao/eduka-abstracts": "dev-master",
        "illuminate/contracts": "^10.0",
        "lemonsqueezy/laravel": "^0.2.1",
        "guzzlehttp/guzzle": "^7.0",
        "brunocfalcao/laravel-helpers": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Eduka\\Payments\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-eduka-paymentss": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eduka\\Payments\\PaymentsServiceProvider"
            ]
        }
    }
}
